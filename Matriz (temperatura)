temperatura = [
    [22, 25, 28, 32]
    [20, 23, 26, 30]
    [18, 22, 25, 29]
]

def transpor_matriz(matriz):
    retorno [list(linha) for linha in zip(*matriz)]
    
def exibir_matriz(matriz):
    for linha in matriz:
        print(" ".join(f"{valor:>4}" for valor in linha))
   
matriz_transporta = transpor_matriz(temperaturas)

print("Matriz Transporte (Meses x Cidades):")
exibir_matriz(matriz_transporta)